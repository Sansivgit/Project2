<h2>Student Management System</h2>
<P><a href="/adduser" class="addbtn">Add User</a></P>



<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Location</th>
            <th>email</th>
            <th>DOB</th>
            <th>Education</th>
            <th>Action</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            <td>{{this.ID}}</td>
            <td>{{this.FirstName}}</td>
            <td>{{this.LastName}}</td>
            <td>{{this.Location}}</td>
            <td>{{this.Email}}</td>
            <td>{{this.DOB}}</td>
            <td>{{this.Education}}</td>
            <td><a href="/edituser/{{this.ID}}" class="edit">Edit</a></td>
            <td><a onclick="return confirm('Are you sure?')" href="/deleteuser/{{this.ID}}" class="delete">Delete</a></td>

        </tr>
        {{/each}}
    </tbody>
</table>